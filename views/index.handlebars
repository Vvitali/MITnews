<h2>Hi!</h2>

{{#each news}}
<div class="card left">
	<div class="card-image">
		<img src="{{this.photoUrl}}">
		<span class="card-title">{{this.title}}</span>
	</div>
	<div class="card-content">
		<p>{{this.body}}</p>
	</div>
	<div class="card-action">
		<a href="http://news.mit.edu{{this.url}}">Link</a>
	</div>
</div>
{{/each}}

